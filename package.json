{
  "name": "billet",
  "version": "0.1.0",
  "repository": "https://github.com/onethirtyfive/billet",
  "private": false,
  "type": "module",
  "description": "Simple integrated application event logging",
  "main": "billet.js",
  "license": "MIT",
  "contributors": [
    "Joshua Morris <joshua.a.morris@gmail.com>",
    "Joel Diharce <joeldiharce@me.com>"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "readJson": "TS_NODE_PROJECT=./tsconfig.json pushd examples && node --loader ts-node/esm ./readJson.ts ./data/json/events.json",
    "readMsgpack": "TS_NODE_PROJECT=./tsconfig.json pushd examples && node --loader ts-node/esm ./readMsgpack.ts ./data/msgpack/events.msgpack",
    "writeMsgpack": "TS_NODE_PROJECT=./tsconfig.json pushd examples && node --loader ts-node/esm ./writeMsgpack.ts ./data/json/events.json ./data/msgpack/events.msgpack",
    "test": "jest --coverage --watch"
  },
  "dependencies": {
    "immer": "^9.0.6",
    "jsonata": "^1.8.5",
    "object-hash": "^2.2.0",
    "uuid": "^8.3.2",
    "@types/uuid": "^8.3.3",
    "@types/object-hash": "^2.2.1",
    "@msgpack/msgpack": "^2.7.1"
  },
  "devDependencies": {
    "jest": "^27.1.2",
    "typescript": "^4.5.3",
    "ts-standard": "^11.0.0",
    "ts-node": "^10.4.0",
    "ts-jest": "^27.1.2",
    "@types/jest": "^27.0.3"
  }
}
