  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch 'Plan event (multijson)''",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/examples/planEvent.ts",
      "args": [
        "multijson"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "name": "Launch 'Plan event (msgpack)'",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/examples/planEvent.ts",
      "args": [
        "multijson"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "name": "Launch 'Generate msgpack'",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/examples/generateMsgpack.ts",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}