{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch 'Read Json''",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/examples/readJson.ts",
      "args": [
        "${workspaceFolder}/examples/data/json/events.json"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "name": "Launch 'Read Msgpack'",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/examples/readMsgpack.ts",
      "args": [
        "${workspaceFolder}/examples/data/msgpack/events.msgpack"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "name": "Launch 'Write Msgpack'",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/examples/writeMsgpack.ts",
      "args": [
        "${workspaceFolder}/examples/data/json/events.json",
        "${workspaceFolder}/examples/data/msgpack/events.msgpack"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}